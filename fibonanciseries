package sai.java;
import java.util.*;
public class fibonanciseries {
	public static void main(String[] args) {
		int n =10,a=0,b=1;
		System.out.println("Fibonanci series first" +n+" terms");
		
		for(int i = 1; i <= n; i++) {
			System.out.println(a +" ");
			
		
			int c= a + b;
			a = b;
			b = c;
			System.out.println(c);
		}
			
					
		}
	}


